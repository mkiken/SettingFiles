"*":
  "*":
    core:
      audioBeep: false
      disabledPackages: [
        "metrics"
      ]
      themes: [
        "atom-dark-ui"
        "tomorrow-night-eighties-syntax"
      ]
    editor:
      fontSize: 12
      scrollPastEnd: true
      showIndentGuide: true
      showInvisibles: true
      softWrap: true
      softWrapAtPreferredLineLength: true
    "exception-reporting":
      userId: "0a7c8ad7-7803-a619-cd7e-9c76b0560ed6"
    linter:
      lintOnChangeInterval: 3000
      statusBar: "Show error if the cursor is in range"
    "markdown-preview":
      breakOnSingleNewline: true
    metrics:
      userId: "4b0dd0d8e33ff1756775a073b0cdb1d5e4bd19e1"
    "tree-view":
      hideVcsIgnoredFiles: true
    welcome:
      showOnStartup: false
    whitespace:
      removeTrailingWhitespace: false
  "advanced-open-file":
    fuzzyMatch: true
    helmDirSwitch: true
  "autocomplete-plus":
    confirmCompletion: "enter"
  "clang-format":
    fallbackStyle: "google"
  core:
    closeDeletedFileTabs: true
    disabledPackages: [
      "go-debug"
      "go-plus"
      "go-signature-statusbar"
      "github"
    ]
    telemetryConsent: "limited"
    themes: [
      "atom-material-ui"
      "batman-syntax"
    ]
    titleBar: "custom"
  "cursor-history":
    excludeClosedBuffer: true
  editor:
    fontSize: 11
    invisibles: {}
    nonWordCharacters: "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-â€¦_"
    scrollPastEnd: true
    showInvisibles: true
    softWrap: true
    zoomFontWhenCtrlScrolling: true
  "exception-reporting":
    userId: "ab2ede93-4a6d-ac5c-e79c-362a6c7e1cd8"
  fonts:
    fontFamily: "Source Code Pro Medium"
  "git-diff":
    showIconsInEditorGutter: true
  jumpy:
    highContrast: true
  linter: {}
  "linter-ui-default":
    panelHeight: 61
    showPanel: true
  "one-dark-ui": {}
  "tree-view":
    autoReveal: true
    focusOnReveal: false
  "vim-mode-plus":
    automaticallyEscapeInsertModeOnActivePaneItemChange: true
    clearHighlightSearchOnResetNormalMode: true
    clearPersistentSelectionOnResetNormalMode: true
    hideStatusBarOnMaximizePane: false
    hideTabBarOnMaximizePane: false
    highlightSearch: true
    incrementalSearch: true
    showHoverSearchCounter: true
    statusBarModeStringStyle: "long"
    useClipboardAsDefaultRegister: false
    useSmartcaseForSearch: true
    useSmartcaseForSearchCurrentWord: true
  welcome:
    showOnStartup: false
  whitespace:
    removeTrailingWhitespace: false
