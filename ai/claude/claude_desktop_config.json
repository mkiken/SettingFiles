{
  "globalShortcut": "",
  "mcpServers": {
    "Context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "serena": {
      "command": "/usr/local/bin/uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ]
    },
    "gitmcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/docs"
      ]
    },
    "obsidian-mcp-tools": {
      "command": "/usr/local/bin/mcp-server",
      "env": {
        "OBSIDIAN_API_KEY": "a29db6e38ed250507f1025f9e21ceb10fef2afbf0f0b58ecb7ba6fdca6b37945"
      }
    }
  },
  "isUsingBuiltInNodeForMcp": false
}